{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "create-block/rrze-designsystem",
    "title": "Designsystem RRZE",
    "version": "0.0.1",
    "description": "A block to show a design system",
    "category": "widgets",
    "icon": "admin-appearance",
    "keywords": [
        "design",
        "designsystem"
    ],
    "textdomain": "rrze-designsystem",
    "attributes": {
        "content": {
            "type": "string",
            "source": "html",
            "selector": "pre code"
        },
        "alignment": {
            "type": "string",
            "default": "none"
        }
    },
    "editorScript": "file:./index.js",
    "script": "file:./frontend.js",
    "style": "file:./style-index.css",
    "editorStyle": "file:./editor-style.css"
}